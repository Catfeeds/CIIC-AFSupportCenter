server:
  port: 6012
  context-path: /

spring:
  datasource:
    type: com.alibaba.druid.pool.DruidDataSource
    driver-class-name: com.mysql.jdbc.Driver
    url: jdbc:mysql://172.16.9.15:3306/gtobusinessdb?characterEncoding=utf8&useSSL=true
    username: root
    password: a111111
  application:
    name: support-center-soc-job
  profiles:
    active: develop
  cloud:
    stream:
      kafka:
        binder:
          brokers: 172.16.9.17:9092,172.16.9.18:9092
          zk-nodes: 172.16.9.19:2181,172.16.9.20:2181,172.16.9.21:2181
      bindings:
        soc_report_input_channel:
          destination: af_soc_report_generate #topic name
          content-type: application/json
          group: ${spring.application.name} #consumer group
          consumer:
            partitioned: true

      instance-count: 2
      instance-index: 1
eureka:
  client:
    serviceUrl:
      defaultZone: http://172.16.9.20:9602/eureka
  instance:
    lease-expiration-duration-in-seconds: 30
    lease-renewal-interval-in-seconds: 40
    prefer-ip-address: true
    instance-id: ${spring.cloud.client.ipAddress}:${server.port}
mongo:
  host: 172.16.9.16
  port: 27017
  database: payroll_db
  userName: ciicit
  pwd: ciicit!#%


mybatis-plus:
  mapper-locations: classpath*:mapper/**.xml
  type-aliases-package: com.ciicsh.gto.afsupportcenter.socjob.entity
  plugins:
    - catMybatisInterceptor

  configuration:
      log-impl: org.apache.ibatis.logging.stdout.StdOutImpl
      map-underscore-to-camel-case: true
      cache-enabled: false
logging:
  config: classpath:logback.xml
xxl:
  job:
    admin:
      addresses: http://127.0.0.1:8080/xxl-job-admin
    executor:
      appname: soc-job-executor
      ip:
      port: 9999
      logpath: /data/applogs/xxl-job/jobhandler
      logretentiondays: -1
    accessToken: