<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ciicsh.gto.afsupportcenter.healthmedical.queryservice.dao.InsurancePolicyNumMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMapBO" type="com.ciicsh.gto.afsupportcenter.healthmedical.queryservice.entity.bo.InsurancePolicyNumBO">
		<id column="insurance_policy_num_id" property="insurancePolicyNumId" />
		<result column="insurance_policy_num_name" property="insurancePolicyNumName" />
		<result column="insurance_policy_id" property="insurancePolicyId" />
		<result column="insurance_policy_num_start_date" property="insurancePolicyNumStartDate" />
		<result column="insurance_policy_num_end_date" property="insurancePolicyNumEndDate" />
		<result column="payment_cycle" property="paymentCycle" />
		<result column="payment_calculation_rule" property="paymentCalculationRule" />
		<result column="is_active" property="isActive" />
		<result column="created_time" property="createdTime" />
		<result column="modified_time" property="modifiedTime" />
		<result column="created_by" property="createdBy" />
		<result column="modified_by" property="modifiedBy" />
		<collection property="insurancePolicyNumCalculationRulePOList" column="rule_id"
					javaType="ArrayList"  ofType="com.ciicsh.gto.afsupportcenter.healthmedical.queryservice.entity.po.InsurancePolicyNumCalculationRulePO">
		</collection>
	</resultMap>

	    <select id="getInsurancePolicyNumByID" resultMap="BaseResultMapBO">
		   select `insurance_policy_num_id`, `insurance_policy_num_name`, `insurance_policy_id`, `insurance_policy_num_start_date`, `insurance_policy_num_end_date`, `payment_cycle`,
                   `payment_calculation_rule`, `is_active`, `created_time`, `modified_time`, `created_by`, `modified_by`
		   from hm_insurance_policy_num
		   where insurance_policy_num_id = #{insurance_policy_num_id,jdbcType=INTEGER}
	    </select>



</mapper>
