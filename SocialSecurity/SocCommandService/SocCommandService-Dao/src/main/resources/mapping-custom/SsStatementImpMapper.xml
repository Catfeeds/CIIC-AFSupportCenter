<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ciicsh.gto.afsupportcenter.socialsecurity.soccommandservice.dao.SsStatementImpMapper">


    <select id="getImpDetailByStatementId" parameterType="java.lang.Long" resultMap="BaseResultMap">
        SELECT
            *
        FROM
            ss_statement_imp T1
        WHERE
            T1.is_active = '1'
            AND T1.statement_id = #{statementId,jdbcType=BIGINT}
    </select>



</mapper>
